.container{
  display: flex;
  flex-direction: column;
  width:200rpx;
}
.item-image{
  width:100%;
  height:270rpx;
  margin-bottom: 20rpx;
}
.item-title{
  font-size:24rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  word-break:break-all;
}
.rate-container{
  display: flex;
  flex-direction: row;
  align-items: baseline;
}
.score{
  margin-left:20rpx;
}